@model List<Project_3._0.Model.Domain.Object>

@using Project_3._0.Helpers.Html
<body class="properties-page">
	<main class="main">

		<!-- Page Title -->
		<div class="page-title" data-aos="fade">
			<div class="heading">
				<div class="container">
					<div class="row d-flex justify-content-center text-center">
						<div class="col-lg-8">
							<h1>@ViewBag.Object</h1>
							<p class="mb-0">@ViewBag.Desc</p>
						</div>
					</div>
				</div>
			</div>
			<nav class="breadcrumbs">
				<div class="container">
					<ol>
						<li><a asp-controller="Home" asp-action="Home">Главная страница</a></li>
						<li class="current">@ViewBag.Object</li>
					</ol>
				</div>
			</nav>
		</div><!-- End Page Title -->
		<!-- Real Estate Section -->
		<section id="real-estate" class="real-estate section">

			<div class="container">

				<div class="row gy-4">

					@{
						for (int i = 0; i < Model.Count ; i++)
						{
							//new Project_3._0.Model.Domain.Object() { Id = 3, Citi = "London", Street = "Time Scver", House = 12, Apartment = 4, Floor = 2, AreaHouse = 45, Rooms = 2, Price = 123456, Desc = "" }
							@Html.ObjectItem(Model[i], (i+1) * 100)
						}
					}
				</div>

			</div>

		</section><!-- /Real Estate Section -->
	</main>
	<!-- Scroll Top -->
	<a href="#" id="scroll-top" class="scroll-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

	<!-- Preloader -->
	<div id="preloader"></div>
</body>